@article{demorestShapiroDelayMeasurement2010,
  title = {Shapiro delay measurement of a two solar mass neutron star},
  author = {Demorest, Paul and Pennucci, Tim and Ransom, Scott and Roberts, Mallory and Hessels, Jason},
