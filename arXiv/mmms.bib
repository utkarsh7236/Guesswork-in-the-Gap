  file = {/Users/utkarsh/Zotero/storage/Y7XEY4KT/Golomb et al. - 2025 - Using Equation of State Constraints to Classify Low-Mass Compact Binary Mergers.pdf;/Users/utkarsh/Zotero/storage/A68SRWIG/2403.html}
}

@article{heinzelPixelPopHighResolution2025,
  title = {{{PixelPop}}: {{High Resolution Nonparameteric Inference}} of {{Gravitational-Wave Populations}} in {{Multiple Dimensions}}},
