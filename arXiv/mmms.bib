  eprint = {2006.12611},
  primaryclass = {astro-ph},
  pages = {L44},
  issn = {2041-8205, 2041-8213},
  doi = {10.3847/2041-8213/ab960f},
  urldate = {2025-08-29},
  abstract = {We report the observation of a compact binary coalescence involving a 22.2 - 24.3 \$M\_\textbraceleft\textbackslash odot\textbraceright\$ black hole and a compact object with a mass of 2.50 - 2.67 \$M\_\textbraceleft\textbackslash odot\textbraceright\$ (all measurements quoted at the 90\$\textbackslash\%\$ credible level). The gravitational-wave signal, GW190814, was observed during LIGO's and Virgo's third observing run on August 14, 2019 at 21:10:39 UTC and has a signal-to-noise ratio of 25 in the three-detector network. The source was localized to 18.5 deg\$\textasciicircum 2\$ at a distance of \$241\textasciicircum\textbraceleft +41\textbraceright\_\textbraceleft -45\textbraceright\$ Mpc; no electromagnetic counterpart has been confirmed to date. The source has the most unequal mass ratio yet measured with gravitational waves, \$0.112\textasciicircum\textbraceleft +0.008\textbraceright\_\textbraceleft -0.009\textbraceright\$, and its secondary component is either the lightest black hole or the heaviest neutron star ever discovered in a double compact-object system. The dimensionless spin of the primary black hole is tightly constrained to \$\textbackslash leq 0.07\$. Tests of general relativity reveal no measurable deviations from the theory, and its prediction of higher-multipole emission is confirmed at high confidence. We estimate a merger rate density of 1-23 Gpc\$\textasciicircum\textbraceleft -3\textbraceright\$ yr\$\textasciicircum\textbraceleft -1\textbraceright\$ for the new class of binary coalescence sources that GW190814 represents. Astrophysical models predict that binaries with mass ratios similar to this event can form through several channels, but are unlikely to have formed in globular clusters. However, the combination of mass ratio, component masses, and the inferred merger rate for this event challenges all current models for the formation and mass distribution of compact-object binaries.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - High Energy Astrophysical Phenomena,General Relativity and Quantum Cosmology},
  file = {/Users/utkarsh/Zotero/storage/UHQKV978/Collaboration et al. - 2020 - GW190814 Gravitational Waves from the Coalescence of a 23 M$_odot$ Black Hole with a 2.6 M$_odot$.pdf;/Users/utkarsh/Zotero/storage/XHHZFDAI/2006.html}
}

@article{collaborationGWTC1GravitationalWaveTransient2019,
  title = {{{GWTC-1}}: {{A Gravitational-Wave Transient Catalog}} of {{Compact Binary Mergers Observed}} by {{LIGO}} and {{Virgo}} during the {{First}} and {{Second Observing Runs}}},
  shorttitle = {{{GWTC-1}}},
  author = {Abbott, B. P. and Abbott, R. and Abbott, T. D. and Abraham et al., S.},
  year = 2019,
