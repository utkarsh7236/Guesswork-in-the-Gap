}

@article{chawlaGaiaMayDetect2022,
  title = {Gaia {{May Detect Hundreds}} of {{Well-characterized Stellar Black Holes}}},
  author = {Chawla, Chirag and Chatterjee, Sourav and Breivik, Katelyn and Moorthy, Chaithanya Krishna and Andrews, Jeff J. and Sanderson, Robyn E.},
  year = 2022,
  month = may,
  journal = {The Astrophysical Journal},
  volume = {931},
  number = {2},
  pages = {107},
  publisher = {The American Astronomical Society},
  issn = {0004-637X},
  doi = {10.3847/1538-4357/ac60a5},
  urldate = {2025-08-29},
  abstract = {Detection of black holes (BHs) with detached luminous companions (LCs) can be instrumental in connecting the BH properties with their progenitors since the latter can be inferred from the observable properties of the LC. Past studies showed the promise of Gaia astrometry in detecting BH--LC binaries. We build on these studies by (1) initializing the zero-age binary properties based on realistic, metallicity-dependent star formation history in the Milky Way (MW); (2) evolving these binaries to current epoch to generate realistic MW populations of BH--LC binaries; (3) distributing these binaries in the MW, preserving the complex age--metallicity-Galactic position correlations; (4) accounting for extinction and reddening using three-dimensional dust maps; and (5) examining the extended Gaia mission's ability to resolve BH--LC binaries. We restrict ourselves to detached BH--LC binaries with orbital period P orb {$\leq$} 10 yr such that Gaia can observe at least one full orbit. We find that (1) the extended Gaia mission can astrometrically resolve {$\sim$}30--300 detached BH--LC binaries depending on our assumptions of supernova physics and astrometric detection threshold; (2) Gaia's astrometry alone can indicate BH candidates for {$\sim$}10--100 BH--LC binaries by constraining the dark primary mass {$\geq$}3 M {$\odot$}; and (3) distributions of observables, including orbital periods, eccentricities, and component masses, are sensitive to the adopted binary evolution model and hence can directly inform binary evolution models. Finally, we comment on the potential to further characterize these BH binaries through radial velocity measurements and observation of X-ray counterparts.},
  langid = {english},
  file = {/Users/utkarsh/Zotero/storage/QQTL2D57/Chawla et al. - 2022 - Gaia May Detect Hundreds of Well-characterized Stellar Black Holes.pdf}
}

@article{collaborationAdvancedLIGO2015,
  title = {Advanced {{LIGO}}},
  author = {Collaboration, The LIGO Scientific},
  year = 2015,
  month = apr,
  journal = {Classical and Quantum Gravity},
  volume = {32},
  number = {7},
  eprint = {1411.4547},
  primaryclass = {gr-qc},
  pages = {074001},
  issn = {0264-9381, 1361-6382},
  doi = {10.1088/0264-9381/32/7/074001},
  urldate = {2025-08-29},
  abstract = {The Advanced LIGO gravitational wave detectors are second generation instruments designed and built for the two LIGO observatories in Hanford, WA and Livingston, LA. The two instruments are identical in design, and are specialized versions of a Michelson interferometer with 4 km long arms. As in initial LIGO, Fabry-Perot cavities are used in the arms to increase the interaction time with a gravitational wave, and power recycling is used to increase the effective laser power. Signal recycling has been added in Advanced LIGO to improve the frequency response. In the most sensitive frequency region around 100 Hz, the design strain sensitivity is a factor of 10 better than initial LIGO. In addition, the low frequency end of the sensitivity band is moved from 40 Hz down to 10 Hz. All interferometer components have been replaced with improved technologies to achieve this sensitivity gain. Much better seismic isolation and test mass suspensions are responsible for the gains at lower frequencies. Higher laser power, larger test masses and improved mirror coatings lead to the improved sensitivity at mid- and high- frequencies. Data collecting runs with these new instruments are planned to begin in mid-2015.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Instrumentation and Methods for Astrophysics,General Relativity and Quantum Cosmology,Physics - Instrumentation and Detectors},
  file = {/Users/utkarsh/Zotero/storage/HK8QXBEE/Collaboration - 2015 - Advanced LIGO.pdf;/Users/utkarsh/Zotero/storage/KC6R5DVM/1411.html}
}

@article{collaborationGW170817ObservationGravitational2017,
  title = {{{GW170817}}: {{Observation}} of {{Gravitational Waves}} from a {{Binary Neutron Star Inspiral}}},
  shorttitle = {{{GW170817}}},
  author = {Collaboration, The LIGO Scientific and Collaboration, The Virgo},
  year = 2017,
  month = oct,
  journal = {Physical Review Letters},
  volume = {119},
  number = {16},
  eprint = {1710.05832},
  primaryclass = {gr-qc},
  pages = {161101},
  issn = {0031-9007, 1079-7114},
  doi = {10.1103/PhysRevLett.119.161101},
  urldate = {2025-08-29},
  abstract = {On August 17, 2017 at 12:41:04 UTC the Advanced LIGO and Advanced Virgo gravitational-wave detectors made their first observation of a binary neutron star inspiral. The signal, GW170817, was detected with a combined signal-to-noise ratio of 32.4 and a false-alarm-rate estimate of less than one per \$8.0\textbackslash times10\textasciicircum 4\$ years. We infer the component masses of the binary to be between 0.86 and 2.26 \$M\_\textbackslash odot\$, in agreement with masses of known neutron stars. Restricting the component spins to the range inferred in binary neutron stars, we find the component masses to be in the range 1.17 to 1.60 \$M\_\textbackslash odot\$, with the total mass of the system \$2.74\textasciicircum\textbraceleft +0.04\textbraceright\_\textbraceleft -0.01\textbraceright\textbackslash,M\_\textbackslash odot\$. The source was localized within a sky region of 28 deg\$\textasciicircum 2\$ (90\% probability) and had a luminosity distance of \$40\textasciicircum\textbraceleft +8\textbraceright\_\textbraceleft -14\textbraceright\$ Mpc, the closest and most precisely localized gravitational-wave signal yet. The association with the gamma-ray burst GRB 170817A, detected by Fermi-GBM 1.7 s after the coalescence, corroborates the hypothesis of a neutron star merger and provides the first direct evidence of a link between these mergers and short gamma-ray bursts. Subsequent identification of transient counterparts across the electromagnetic spectrum in the same location further supports the interpretation of this event as a neutron star merger. This unprecedented joint gravitational and electromagnetic observation provides insight into astrophysics, dense matter, gravitation and cosmology.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - High Energy Astrophysical Phenomena,General Relativity and Quantum Cosmology},
  file = {/Users/utkarsh/Zotero/storage/A5HGGV35/Collaboration and Collaboration - 2017 - GW170817 Observation of Gravitational Waves from a Binary Neutron Star Inspiral.pdf;/Users/utkarsh/Zotero/storage/IUHU7JEK/1710.html}
}

