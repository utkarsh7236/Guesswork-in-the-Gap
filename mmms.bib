  doi = {10.1103/PhysRevD.101.123007},
  urldate = {2025-08-31},
  abstract = {Observations of neutron stars, whether in binaries or in isolation, provide information about the internal structure of the most extreme material objects in the Universe. In this work, we combine information from recent observations to place joint constraints on the properties of neutron star matter. We use (i) lower limits on the maximum mass of neutron stars obtained through radio observations of heavy pulsars, (ii) constraints on tidal properties inferred through the gravitational waves neutron star binaries emit as they coalesce, and (iii) information about neutron stars' masses and radii obtained through X-ray emission from surface hot spots. In order to combine information from such distinct messengers while avoiding the kind of modeling systematics intrinsic to parametric inference schemes, we employ a nonparametric representation of the neutron-star equation of state based on Gaussian processes conditioned on nuclear theory models. We find that existing astronomical observations imply \$R\_\textbraceleft 1.4\textbraceright =12.32\textasciicircum\textbraceleft +1.09\textbraceright\_\textbraceleft -1.47\textbraceright\textbackslash,\textbackslash mathrm\textbraceleft km\textbraceright\$ for the radius of a \$1.4\textbackslash,M\_\textbraceleft\textbackslash odot\textbraceright\$ neutron star and \$p(2\textbackslash rho\_\textbackslash mathrm\textbraceleft nuc\textbraceright )=3.8\textasciicircum\textbraceleft +2.7\textbraceright\_\textbraceleft -2.9\textbraceright\textbackslash times10\textasciicircum\textbraceleft 34\textbraceright\textbackslash,\textbackslash mathrm\textbraceleft dyn\textbraceright/\textbackslash mathrm\textbraceleft cm\textbraceright\textasciicircum 2\$ for the pressure at twice nuclear saturation density at the 90\% credible level. The upper bounds are driven by the gravitational wave observations, while X-ray and heavy pulsar observations drive the lower bounds. Additionally, we compute expected constraints from potential future astronomical observations and find that they can jointly determine \$R\_\textbraceleft 1.4\textbraceright\$ to \$\textbraceleft\textbackslash cal\textbraceleft O\textbraceright\textbraceright (1)\textbackslash,\textbackslash mathrm\textbraceleft km\textbraceright\$ and \$p(2\textbackslash rho\_\textbackslash mathrm\textbraceleft nuc\textbraceright )\$ to 80\% relative uncertainty in the next five years.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - High Energy Astrophysical Phenomena,General Relativity and Quantum Cosmology,Nuclear Theory},
  file = {/Users/utkarsh/Zotero/storage/FR2RN4P6/Landry et al. - 2020 - Nonparametric constraints on neutron star matter with existing and upcoming gravitational wave and p.pdf;/Users/utkarsh/Zotero/storage/QRLL86XI/2003.html}
}

@article{legredImpactPSR$mathrmJ0740+6620$2021,
  title = {Impact of the {{PSR}} \$\textbackslash mathrm\textbraceleft{{J}}\textbraceright 0740+6620\$ radius constraint on the properties of high-density matter},
  author = {Legred, Isaac and Chatziioannou, Katerina and Essick, Reed and Han, Sophia and Landry, Philippe},
