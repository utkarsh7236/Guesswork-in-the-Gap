@article{fattoyevGW190814Impact262020,
  title = {{{GW190814}}: {{Impact}} of a 2.6 solar mass neutron star on nucleonic equations of state},
  shorttitle = {{{GW190814}}},
  author = {Fattoyev, F. J. and Horowitz, C. J. and Piekarewicz, J. and Reed, Brendan},
  year = 2020,
  month = dec,
