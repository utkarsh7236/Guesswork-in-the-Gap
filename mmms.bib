@article{essickNonparametricInferenceNeutron2020,
  title = {Nonparametric {{Inference}} of {{Neutron Star Composition}}, {{Equation}} of {{State}}, and {{Maximum Mass}} with {{GW170817}}},
  author = {Essick, Reed and Landry, Philippe and Holz, Daniel E.},
  year = 2020,
  month = mar,
  journal = {Physical Review D},
