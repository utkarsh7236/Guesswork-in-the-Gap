@article{gaoFormationMassgapBlack2022,
  title = {Formation of mass-gap black holes from neutron star {{X-ray}} binaries with super-{{Eddington}} accretion},
  author = {Gao, Shi-Jie and Li, Xiang-Dong and Shao, Yong},
  year = 2022,
  month = jul,
  journal = {Monthly Notices of the Royal Astronomical Society},
  volume = {514},
